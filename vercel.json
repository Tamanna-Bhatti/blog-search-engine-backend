{
  "installCommand": "npm install --legacy-peer-deps",
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "src/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/search(.*)", "dest": "/index.js" },
    { "src": "/crawl(.*)", "dest": "/index.js" },
    { "src": "/classify-url(.*)", "dest": "/index.js" },
    { "src": "/health", "dest": "/index.js" },
    { "src": "/(.*)", "dest": "/index.js" }
  ]
}
